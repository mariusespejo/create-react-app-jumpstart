{
  "name": "create-react-app-jumpstart",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.13",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "cross-env": "^5.0.5",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "fetch-mock": "^5.13.0",
    "jest-enzyme": "^4.0.1",
    "json-server": "^0.12.0",
    "node-sass-chokidar": "0.0.3",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "npm run test-coverage",
    "start": "npm-run-all -p watch-css start-js",
    "dev": "cross-env REACT_APP_MOCK_API=true npm-run-all -p mock-api start",
    "start-js": "react-scripts start",
    "mock-api": "nodemon src/api/mockApi.js",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build": "npm run build-css && react-scripts build",
    "test": "npm run build-css -- --quiet && react-scripts test --env=jsdom",
    "test-coverage": "npm test -- --verbose --coverage",
    "eject": "react-scripts eject"
  },
  "proxy": {
    "/api": {
      "target": "<add real api here>",
      "secure": false,
      "changeOrigin": true,
      "ws": true,
      "xfwd": true
    },
    "/mockApi": {
      "target": "http://localhost:3001/",
      "ws": true
    }
  }
}
